{
    "name": "postcss-logical-properties-polyfill",
    "version": "0.0.1",
    "description": "PostCSS plugin that polyfill Mozilla's Bi-directional CSS proposal to support direction-sensitive rules, a.k.a Left-To-Right (LTR) and Right-To-Left (RTL), in single syntax",
    "keywords": [
        "postcss",
        "css",
        "postcss-plugin",
        "bidi",
        "rtl",
        "ltr"
    ],
    "author": "ErickSkrauch <erickskrauch@ely.by>",
    "license": "MIT",
    "repository": "erickskrauch/postcss-bidirection",
    "bugs": {
        "url": "https://github.com/erickskrauch/postcss-bidirection/issues"
    },
    "homepage": "https://github.com/erickskrauch/postcss-bidirection",
    "engines": {
        "node": ">=12"
    },
    "devDependencies": {
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.37",
        "jest": "^26.6.3",
        "jest-file-snapshot": "satya164/jest-file-snapshot#2b157be",
        "postcss": "^8",
        "prettier": "^2.2.1",
        "ts-jest": "^26.5.4",
        "typescript": "^4.2.3"
    },
    "peerDependencies": {
        "postcss": "^8"
    },
    "scripts": {
        "build": "yarn clean && tsc -p tsconfig-build.json",
        "test": "jest",
        "tsc": "tsc --noEmit --pretty",
        "clean": "rimraf dist"
    },
    "main": "dist/plugin.js"
}
